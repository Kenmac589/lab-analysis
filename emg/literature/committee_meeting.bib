	issn = {1469-7793},
	journal = {The Journal of Physiology},
	keywords = {locomotion, proprioception, sensory feedback, Egr3, Lyapunov, muscle synergies, Lab Readings, ??; included},
	language = {en},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1113/JP277515},
	number = {12},
	pages = {3147--3165},
	title = {Modular organization of murine locomotor pattern in the presence and absence of sensory feedback from muscle spindles},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1113/JP277515},
	volume = {597},
	year = {2019},
