	issn = {14396319},
	journal = {European Journal of Applied Physiology},
	keywords = {Humans, Medical Sciences, Biology--Physiology, RAP, Rosa; included},
	language = {English},
	month = jan,
	note = {Num Pages: 167-73 Place: Heidelberg, Netherlands Publisher: Springer Nature B.V.},
	number = {2},
	pages = {167--73},
	title = {The contribution of mechanoreceptive sensation on stability and adaptation in the young and elderly},
	url = {http://www.proquest.com/docview/203324779/abstract/EBD28967B334A0APQ/1},
