	issn = {0002-0729},
	journal = {Age and Ageing},
	keywords = {included},
	month = jul,
	number = {4},
	pages = {292--299},
	title = {Sensori-motor Function, Gait Patterns and Falls in Community-dwelling Women},
	url = {https://doi.org/10.1093/ageing/25.4.292},
	volume = {25},
	year = {1996},
	bdsk-url-1 = {https://doi.org/10.1093/ageing/25.4.292}}

