@article{Akay2014,
	abstract = {Significance
            Terrestrial locomotion is thought to be generated by the actions of a circuit of interconnected interneurons (central pattern generator) in the spinal cord that drive the patterned activity of pools of motor neurons, causing sequential contraction of dozens of leg muscles. Sensory feedback exerts a strong modulatory influence on this pattern; nevertheless, it remains unclear whether sensory feedback also plays a role in the generation of the normal locomotor pattern. Through the use of a combination of electrophysiology, behavior, and mouse genetics, we provide evidence that the absence of proprioceptive sensory feedback degrades locomotor pattern, indicating that proprioceptive feedback is required for the construction of locomotor pattern.
          , 
            Mammalian locomotor programs are thought to be directed by the actions of spinal interneuron circuits collectively referred to as {\textquotedblleft}central pattern generators.{\textquotedblright} The contribution of proprioceptive sensory feedback to the coordination of locomotor activity remains less clear. We have analyzed changes in mouse locomotor pattern under conditions in which proprioceptive feedback is attenuated genetically and biomechanically. We find that locomotor pattern degrades upon elimination of proprioceptive feedback from muscle spindles and Golgi tendon organs. The degradation of locomotor pattern is manifest as the loss of interjoint coordination and alternation of flexor and extensor muscles. Group Ia/II sensory feedback from muscle spindles has a predominant influence in patterning the activity of flexor muscles, whereas the redundant activities of group Ia/II and group Ib afferents appear to determine the pattern of extensor muscle firing. These findings establish a role for proprioceptive feedback in the control of fundamental aspects of mammalian locomotor behavior.},
	author = {Akay, Turgay and Tourtellotte, Warren G. and Arber, Silvia and Jessell, Thomas M.},
	date-added = {2023-12-02 14:15:08 -0400},
	date-modified = {2023-12-02 14:15:08 -0400},
	doi = {10.1073/pnas.1419045111},
	file = {Akay2014.pdf:/Users/kenzie_mackinnon/Documents/papers/akay-lab/Akay2014.pdf:application/pdf},
	issn = {0027-8424, 1091-6490},
	journal = {Proceedings of the National Academy of Sciences},
	language = {en},
	month = nov,
	number = {47},
	pages = {16877--16882},
	title = {Degradation of mouse locomotor pattern in the absence of proprioceptive sensory feedback},
	url = {https://pnas.org/doi/full/10.1073/pnas.1419045111},
	urldate = {2023-03-06},
	volume = {111},
	year = {2014},
	bdsk-url-1 = {https://pnas.org/doi/full/10.1073/pnas.1419045111},
	bdsk-url-2 = {https://doi.org/10.1073/pnas.1419045111}}

@article{Akay2023,
	abstract = { Among all daily activities, walking accounts for the highest proportion of falls in the elderly (24%), and falls occur more often in the lateral direction (Robinovitch et al., 2013). The compromised proprioceptive and tactile sensation due to aging or illness contributes to falls, although precise mechanisms are not well understood. Previous research has demonstrated the importance of mechanosensory cutaneous information for maintaining lateral stability in humans (Meyer et al., 2004) and cats (Park et al., 2019), but the role of proprioception remains obscure. Mice provide an unprecedented opportunity to investigate the role of proprioception in controlling lateral locomotor stability due to advances in genetic engineering. Crouched locomotion and small size might prevent mice from loosing balance during locomotion even after major disruptions of proprioceptive feedback (Akay et al., 2014) and vestibular function (Murray et al., 2018), whereas larger species like cats and humans experience much greater balance deficits after losing somatosensory feedback (Stapley et al., 2002; Cole, Paillard, 1998). It is still unclear if, how and to what extent mice rely on proprioceptive sensory feedback to maintain lateral stability during locomotion. The purpose of this study was to investigate the role of muscle length-dependent sensory feedback in control of lateral stability during mouse locomotion. We hypothesized that mice lacking the muscle spindles modify locomotor posture and frontal plane kinematics to maintain lateral stability; i.e., muscle spindle afferents participate in regulation of lateral stability of normal mouse locomotion. We determined measures of lateral stability during treadmill locomotion at different speeds in wild-type (WT) mice and in mice that lack proprioceptive muscle spindle feedback, the Egr3 knock out mice (Egr3-KO, Tourtellotte and Milbrandt, 1998). The measures of lateral stability were calculated using the positions of the center of mass (COM), labeled with a marker on the body, and paws recorded by a high-speed video system while animals were locomoting on a transparent treadmill with a mirror underneath. We show that mice without muscle spindle feedback assume a lower body posture. The COM lateral position fluctuates in Egr3-KO mice as much as in WT. The hindlimb step width is lower in Eg3-KO mice than in WT, whereas the forelimb step width is similar. Changes in the height of the body, step width, and lateral CoM fluctuation led to similar measures of lateral stability in WT and Egr3-KO mice during locomotion. We also found correlation between the lateral COM shifts in individual walking cycles with the change in lateral position of the ipsilateral paws in the subsequent cycles regardless of the presence of muscle spindle feedback. In addition, we show that changes in the lateral stability measures in response to sudden and random lateral displacement perturbations in WT and Egr3-KO mice were very similar. Since locomotor adaptations of Egr3-KO mice lead to similar lateral stability measures as in the WT mice, we conclude that muscle spindle afferents participate in regulation of lateral stability during normal locomotion.},
	author = {Turgay Akay and Alexander N. Klishko and Boris I. Prilutsky},
	date-added = {2023-12-02 13:27:59 -0400},
	date-modified = {2023-12-02 13:38:23 -0400},
	journal = {Poster presented at Society for Neuroscience 2023},
	title = {Role of proprioceptive feedback from muscle spindles in lateral postural stability during mouse locomotion Role of proprioceptive feedback from muscle spindles in lateral postural stability during mouse locomotion},
	year = {2023},
	bdsk-file-1 = {}}

